{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${mod_version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "contact": {
    "sources": "${sources}"
  },
  "license": "${license}",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.akicater.fabric.IplaFabric"
    ],
    "client": [
      "com.akicater.fabric.client.IplaFabricClient"
    ],
    "modmenu": [
      "com.akicater.fabric.client.ModMenuIntegration"
    ]
  },
  "mixins": [
    "ipla.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=${java}",
    "architectury": ">=${architectury_api_version}"
  }
}
